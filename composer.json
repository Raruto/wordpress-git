{
    "name": "raruto/wordpress-git",
    "description" : "a simple skeleton repo for managing a WordPress site using: composer, git",
    "type"        : "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-theme/twentyfifteen": "*",
        "wpackagist-plugin/advanced-cron-manager": "2.3.1",
        "wpackagist-plugin/malinky-ajax-pagination": "2.0.1",
        "wpackagist-plugin/akismet": "4.0.3",
        "wpackagist-plugin/all-in-one-wp-migration": "6.65",
        "wpackagist-plugin/bj-lazy-load": "1.0.9",
        "wpackagist-plugin/busted": "1.4",
        "wpackagist-plugin/dewdrop-custom-scrollbar": "1.4",
        "wpackagist-plugin/dont-muck-my-markup": "1.7",
        "wpackagist-plugin/easy-fancybox": "1.6.3",
        "wpackagist-plugin/ewww-image-optimizer": "4.1.1",
        "afragen/github-updater": "7.5.0",
        "wpackagist-plugin/html-editor-syntax-highlighter": "2.2.3",
        "wpackagist-plugin/media-cleaner": "4.5.8",
        "wpackagist-plugin/media-file-renamer": "4.0.3",
        "johnbillion/query-monitor": "2.17.0",
        "wpackagist-plugin/regenerate-thumbnails": "3.0.2",
        "blazersix/satispress": "0.2.3",
        "wpackagist-plugin/scalable-vector-graphics-svg": "3.4",
        "wpackagist-plugin/transients-manager": "1.7.4",
        "wpackagist-plugin/w3-total-cache": "0.9.6",
        "wpackagist-plugin/widget-output-cache": "0.5.2",
        "wpackagist-plugin/wp-optimize": "2.2.2",
        "wpackagist-plugin/wp-piwik": "1.0.19",
        "wpackagist-plugin/wp-full-auto-tags-manager": "2.2",
        "wpackagist-plugin/wordpress-seo": "7.1"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "prefer-stable": true
}
